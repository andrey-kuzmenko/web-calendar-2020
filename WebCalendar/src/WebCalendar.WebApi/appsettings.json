{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Quartz": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": ".\\logs\\log.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=WebCalendarDatabase;Trusted_Connection=True;MultipleActiveResultSets=true",
  "HangfireConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=HangfireDatabase;Trusted_Connection=True;MultipleActiveResultSets=true",
  "Jwt": {
    "Key": "mysupersecret_secretkey!123",
    "Issuer": "WebCalendarServer",
    "Audience": "WebCalendarClient",
    "Lifetime": 1440
  }
}
